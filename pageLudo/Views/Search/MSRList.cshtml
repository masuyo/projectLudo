@model IEnumerable<pageLudo.Models.UserListingData>


@{
    ViewBag.Title = "MultipleProfileSearchResult";
}

@if (Model.Count() > 0)
{
    <table border="1">
        <tr>
            <th>@Html.Display("Username")</th>
            <th>@Html.Display("E-mail address")</th>
        </tr> 
        @foreach (var user in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => user.Username)</td>
                <td>@Html.DisplayFor(m => user.EmailID)</td>
                <th>Datasheet</th>
                <th>Delete</th>
                <th>Edit</th>
                @*<td>
                    @Html.ActionLink("Show", "Details",
                        new { id = grade.ID })
                </td>
                <td>
                    @Html.ActionLink("Remove", "Remove",
                        new { id = grade.ID })
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit",
                        new { id = grade.ID })
                </td>*@
            </tr>
        }
    </table>
}
else
{
    <p>No data in the database!</p>
}